
SELECT A.CRSE_ID, A.CRSE_OFFER_NBR, A.STRM, A.SESSION_CODE, A.CLASS_SECTION, A.SUBJECT, A.CATALOG_NBR, A.DESCR, A.CLASS_NBR, B.DESCR
FROM PS_CLASS_TBL A, PS_TERM_TBL B
WHERE ( A.STRM = B.STRM
   AND A.SESSION_CODE = B.SESSION_CODE
   AND B.INSTITUTION = A.INSTITUTION
   AND B.ACAD_CAREER = A.ACAD_CAREER
   AND A.STRM = '2192'
   AND A.SESSION_CODE = '1')
